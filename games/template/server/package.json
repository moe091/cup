{
  "name": "@cup/template-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json -w",
    "start:watch": "node --watch dist/main.js",
    "start:dev": "pnpm run build:watch & pnpm run start:watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "pnpm run typecheck",
    "format": "prettier --config ../../../apps/api/.prettierrc --check \"src/**/*.ts\"",
    "format:fix": "prettier --config ../../../apps/api/.prettierrc --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.26.1",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prettier": "^3.4.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@cup/template-engine": "workspace:*",
    "@cup/template-shared": "workspace:*",
    "socket.io": "^4.8.3"
  }
}
